{
  "expo": {
    "name": "avalon",
    "slug": "avalon",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "NSCameraUsageDescription": "Avalon needs access to your Camera."
      },
      "bundleIdentifier": "com.sambarilov.avalon"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.sambarilov.avalon",
      "permissions": [
        "android.permission.CAMERA"
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "react-native-vision-camera",
        {
          "cameraPermissionText": "Avalon needs access to your Camera."
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "5655a62a-a47c-4853-a43e-d839dc9eb1ae"
      }
    }
  }
}
